{
  "name": "@cala/remote-collection",
  "version": "0.5.0",
  "description": "A RemoteData Collection class",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run lint && npm run prettier && npm run ava",
    "lint": "tslint -c tslint.json --project tsconfig.json './src/**/*.ts'",
    "ava": "ava",
    "prettier": "prettier --list-different './src/**/*.ts'",
    "prettier:fix": "prettier --write './src/**/*.ts'",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ca-la/remote-collection.git"
  },
  "keywords": [
    "RemoteData",
    "algebraic-data-types",
    "TypeScript",
    "REST",
    "GraphQL",
    "data",
    "fetch"
  ],
  "author": "CALA <devops@ca.la>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/ca-la/remote-collection/issues"
  },
  "homepage": "https://gitlab.com/ca-la/remote-collection#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.116",
    "ava": "^1.1.0",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@cala/remote-data": "^0.1.1",
    "fp-ts": "^1.12.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11"
  }
}
